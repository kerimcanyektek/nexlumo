    <!-- Vendor CSS -->
    <link rel="stylesheet" href="~/vendor/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/vendor/fontawesome-free/css/all.min.css">
    <link rel="stylesheet" href="~/vendor/animate/animate.compat.css">
    <link rel="stylesheet" href="~/vendor/simple-line-icons/css/simple-line-icons.min.css">
    <link rel="stylesheet" href="~/vendor/owl.carousel/assets/owl.carousel.min.css">
    <link rel="stylesheet" href="~/vendor/owl.carousel/assets/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/vendor/magnific-popup/magnific-popup.min.css">

    <!-- Theme CSS -->
    <link rel="stylesheet" href="~/css/theme.css">
    <link rel="stylesheet" href="~/css/theme-elements.css">
    <link rel="stylesheet" href="~/css/theme-blog.css">
    <link rel="stylesheet" href="~/css/theme-shop.css">

    <!-- Demo CSS -->
    <link rel="stylesheet" href="~/css/demos/demo-business-consulting-5.css">

    <!-- Skin CSS -->
    <link id="skinCSS" rel="stylesheet" href="~/css/skins/skin-business-consulting-5.css">

    <!-- Theme Custom CSS -->
    <link rel="stylesheet" href="~/css/custom.css">

    <!-- Head Libs -->
    <script src="~/vendor/modernizr/modernizr.min.js"></script>
    <style>
        .text-12 {
            font-size: 2.4em !important;
        }
    </style>
</head>
<body class="alternative-font-7">
    <div class="body mt-0">
        <div class="notice-top-bar p-relative z-index-1 bg-secondary">

        </div>
        <header id="header" class="header-transparent" data-plugin-options="{'stickyScrollUp': true, 'stickyEnabled': true, 'stickyEnableOnBoxed': true, 'stickyEnableOnMobile': false, 'stickyChangeLogo': true, 'stickyStartAt': 42, 'stickyHeaderContainerHeight': 70}">
            <div class="header-body header-body-bottom-border border-top-0 h-auto">
                <div class="header-container container p-static">
                    <div class="header-row">
                        <div class="header-column">
                            <div class="header-row">
                                <div class="header-logo">
                                    <a href="/Home/Index">
                                        <img src="~/img/demos/business-consulting-5/logo-lifena.png" class="img-fluid" width="123" height="33" alt="" />
                                    </a>
                                </div>
                            </div>
                        </div>
                        <div class="header-column justify-content-end">
                            <div class="header-row">
                                <div class="header-nav header-nav-line header-nav-bottom-line header-nav-bottom-line-effect-1">
                                    <div class="header-nav-main header-nav-main-text-capitalize header-nav-main-square header-nav-main-arrows header-nav-main-dropdown-no-borders header-nav-main-effect-2 header-nav-main-sub-effect-1">
                                        <nav class="collapse">
                                            <ul class="nav nav-pills" id="mainNav">
                                                <li>
                                                    <a href="/Home/Index" class="nav-link active">
                                                        anasayfa
                                                    </a>
                                                </li>
                                                <li>
                                                    <a href="/Home/About" class="nav-link">
                                                        hakkımızda
                                                    </a>
                                                </li>
                                                <li>
            <a href="/Home/Team" class="nav-link">
                ekibimiz
            </a>
        </li>
                                                <li>
                                                    <a class="nav-link" href="/Home/services">
                                                        hizmetler
                                                    </a>
                                                </li>
                                                @*  <li>
            <a class="nav-link" href="/Home/SuccessStories">
                başarı hikayeleri
            </a>
        </li>*@
                                                <li>
                                                    <a class="nav-link" href="/Home/Blog">
                                                        blog
                                                    </a>
                                                </li>
                                                <li>
                                                    <a class="nav-link" href="/Home/Contact">
                                                        İletişim
                                                    </a>
                                                </li>
                                            </ul>
                                        </nav>
                                    </div>
                                </div>

                                <a href="/Home/Contact" class="btn btn-modern btn-primary box-shadow-6 border-0 text-transform-none btn-dash ms-3 d-none d-xl-inline-block">İletişime geçin</a>

                                <button class="btn header-btn-collapse-nav" data-bs-toggle="collapse" data-bs-target=".header-nav-main nav">
                                    <i class="fas fa-bars"></i>
                                </button>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </header>

        <div role="main" class="main">
            @RenderBody()
        </div>

        <footer id="footer" class="position-relative bg-transparent border-top-0">

            <div class="container pt-5-5 pb-3">
                <div class="row gy-5">
                    <div class="col-lg-4">
                        <div class="d-flex align-items-center mb-3">
                            <img src="~/img/demos/business-consulting-5/logo-lifena.png" alt="Logo" style="height: 40px; margin-right: 10px;">
                            <h4 class="text-color-dark font-weight-bold mb-0"></h4>
                        </div>
                        <ul class="social-icons social-icons-clean social-icons-medium">
                            <li class="social-icons-instagram">
                                <a href="http://www.instagram.com/" target="_blank" title="Instagram">
                                    <i class="fab fa-instagram"></i>
                                </a>
                            </li>
                            <li class="social-icons-linkedin">
                                <a href="http://www.linkedin.com/" target="_blank" title="Linkedin">
                                    <i class="fab fa-linkedin-in"></i>
                                </a>
                            </li>
                            <li class="social-icons-facebook">
                                <a href="http://www.facebook.com/" target="_blank" title="Facebook">
                                    <i class="fab fa-facebook-f"></i>
                                </a>
                            </li>
                        </ul>
                    </div>
                    <div class="col-lg-8">
                        <div class="row mb-5-5">
                            <div class="col-lg-6 mb-4 mb-lg-0">
                                <h4 class="text-color-dark font-weight-bold mb-3">NexLumo</h4>
                                <ul class="list list-unstyled columns-lg-2">
                                    <li><a href="/Home/Index" class="text-color-grey text-color-hover-primary">Anasayfa</a></li>
                                    <li><a href="/Home/About" class="text-color-grey text-color-hover-primary">Hakkımızda</a></li>
                                    <li><a href="/Home/Services" class="text-color-grey text-color-hover-primary">Hizmetler</a></li>
                                    <li><a href="/Home/Team" class="text-color-grey text-color-hover-primary">Ekibimiz</a></li>
                                    <li><a href="/Home/Blog" class="text-color-grey text-color-hover-primary">Blog</a></li>
                                    <li><a href="/Home/Contact" class="text-color-grey text-color-hover-primary">İletişim</a></li>
                                </ul>
                            </div>
                            <div class="col-lg-6">
                                <h4 class="text-color-dark font-weight-bold mb-3">Hizmetler</h4>
                                <ul class="list list-unstyled columns-lg-1">
                                    <li><a href="#" class="text-color-grey text-color-hover-primary">Bireysel Motivasyon Danışmanlığı</a></li>
                                    <li><a href="#" class="text-color-grey text-color-hover-primary">Grup Koçluğu</a></li>
                                    <li><a href="#" class="text-color-grey text-color-hover-primary">Hedef Belirleme ve Planlama Atölyeleri</a></li>
                                    <li><a href="#" class="text-color-grey text-color-hover-primary">Online Motivasyon Seminerleri</a></li>
                                </ul>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
            <div class="footer-copyright bg-transparent pb-5 text-center">
                <div class="container">
                    <hr>
                    <div class="row">
                        <div class="col mt-4 mb-4">
                            <p class="text-3 text-dark mb-0 opacity-6">NexLumo | <a href="https://www.grodea.com" target="_blank" class="text-dark text-decoration-none">GRODEA</a> projesidir. Tüm hakları saklıdır. © 2024.</p>

                        </div>
                    </div>
                </div>
            </div>
        </footer>
    </div>

    <!-- Vendor -->
    <script src="~/vendor/jquery/jquery.min.js"></script>
    <script src="~/vendor/jquery.appear/jquery.appear.min.js"></script>
    <script src="~/vendor/jquery.easing/jquery.easing.min.js"></script>
    <script src="~/vendor/jquery.cookie/jquery.cookie.min.js"></script>
    <script src="~/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/vendor/jquery.validation/jquery.validate.min.js"></script>
    <script src="~/vendor/jquery.easy-pie-chart/jquery.easypiechart.min.js"></script>
    <script src="~/vendor/jquery.gmap/jquery.gmap.min.js"></script>
    <script src="~/vendor/lazysizes/lazysizes.min.js"></script>
    <script src="~/vendor/isotope/jquery.isotope.min.js"></script>
